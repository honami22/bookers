<h1>編集画面</h1>
<!--<p>Find me in app/views/books/edit.html.erb</p>-->
<%= form_with model: @book ,url:update_book_path(@book.id),method: :patch do |f| %>


    <h4>タイトル</h4>
    <%= f.text_field :title %>

    <h4>本文</h4>

    <%= f.text_area :body%>

    <%= f.submit 'update' %>

<% end %>
<%= link_to "Back", "/books" %>

<%= link_to "Show", book_path(@book.id)  %>


